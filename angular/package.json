{
  "name": "scoutventure-spa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build --source-map=true --named-chunks=true",
    "sme": "source-map-explorer ./dist/scoutventure-spa/**/*.js",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test",
    "cypress:open": "npx cypress open",
    "cypress": "cypress run",
    "ci:clean": "rimraf ./dist && rimraf ./coverage",
    "ci:test": "ng test --watch=false --browsers=ChromeHeadlessCustom --code-coverage",
    "ci:build": "ng build --configuration production",
    "ci:lint": "ng lint --format json --output-file /eslint/result.json"
  },
  "private": true,
