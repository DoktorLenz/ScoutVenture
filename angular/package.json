{
  "name": "anmelde-tool-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node src/environments/set-env.ts",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build --source-map=true --named-chunks=true",
    "sme": "source-map-explorer ./dist/anmelde-tool-frontend/**/*.js",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test",
    "cypress:open": "npx cypress open",
    "cypress": "cypress run",
    "ci:clean": "rimraf ./dist && rimraf ./coverage",
    "ci:test": "ng test --watch=false --browsers=ChromeHeadlessCustom --code-coverage",
    "ci:build": "ng build --configuration production",
    "ci:lint": "ng lint --format json --output-file /eslint/result.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.3",
    "@angular/common": "^16.1.3",
    "@angular/compiler": "^16.1.3",
    "@angular/core": "^16.1.3",
    "@angular/forms": "^16.1.3",
    "@angular/platform-browser": "^16.1.3",
    "@angular/platform-browser-dynamic": "^16.1.3",
    "@angular/router": "^16.1.3",
    "angular-auth-oidc-client": "16.0.0",
    "ng-mocks": "^14.11.0",
    "primeicons": "^6.0.1",
    "primeng": "^16.0.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.0",
    "uuid": "^9.0.0",
    "zone.js": "^0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.3",
    "@angular-eslint/builder": "16.0.3",
    "@angular-eslint/eslint-plugin": "16.0.3",
    "@angular-eslint/eslint-plugin-template": "16.0.3",
    "@angular-eslint/schematics": "16.0.3",
    "@angular-eslint/template-parser": "16.0.3",
    "@angular/cli": "~16.1.3",
    "@angular/compiler-cli": "^16.1.3",
    "@badeball/cypress-cucumber-preprocessor": "^18.0.1",
    "@cypress/schematic": "^2.5.0",
    "@cypress/webpack-preprocessor": "^5.17.1",
    "@testing-library/angular": "^14.1.1",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jasmine-dom": "^1.3.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/jasmine": "~4.3.5",
    "@types/node": "^20.3.3",
    "@types/testing-library__jasmine-dom": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "colors": "^1.4.0",
    "cypress": "12.16.0",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-testing-library": "^5.11.0",
    "jasmine-core": "~5.0.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "primeflex": "^3.3.1",
    "source-map-explorer": "^2.5.3",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4",
    "webpack": "^5.86.0"
  }
}