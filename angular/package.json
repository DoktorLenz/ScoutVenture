{
  "name": "anmelde-tool-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node src/environments/set-env.ts",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build --source-map=true --named-chunks=true",
    "sme": "source-map-explorer ./dist/anmelde-tool-frontend/**/*.js",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test",
    "cypress:open": "npx cypress open",
    "cypress": "cypress run",
    "ci:clean": "rimraf ./dist && rimraf ./coverage",
    "ci:test": "ng test --watch=false --browsers=ChromeHeadlessCustom --code-coverage",
    "ci:build": "ng build --configuration production",
    "ci:lint": "ng lint --format json --output-file /eslint/result.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "angular-auth-oidc-client": "15.0.5",
    "ng-mocks": "^14.8.0",
    "primeicons": "^6.0.1",
    "primeng": "^15.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.5",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.5",
    "@angular/compiler-cli": "^15.2.0",
    "@badeball/cypress-cucumber-preprocessor": "^17.1.1",
    "@cypress/schematic": "^2.5.0",
    "@cypress/webpack-preprocessor": "^5.17.0",
    "@testing-library/angular": "^13.4.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jasmine-dom": "^1.3.3",
    "@testing-library/user-event": "^14.4.3",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.15.11",
    "@types/testing-library__jasmine-dom": "^1.3.0",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "colors": "^1.4.0",
    "cypress": "latest",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-cypress": "^2.13.2",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-testing-library": "^5.10.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "primeflex": "^3.3.0",
    "source-map-explorer": "^2.5.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4",
    "webpack": "^5.80.0"
  }
}
