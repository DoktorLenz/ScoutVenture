{
  "name": "anmelde-tool-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disable-host-check",
    "build": "ng build --source-map=true --named-chunks=true",
    "sme": "source-map-explorer ./dist/anmelde-tool-frontend/**/*.js",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "ng test",
    "cypress:open": "npx cypress open",
    "cypress": "cypress run",
    "ci:clean": "rimraf ./dist && rimraf ./coverage",
    "ci:test": "ng test --watch=false --browsers=ChromeHeadlessCustom --code-coverage",
    "ci:build": "ng build --configuration production",
    "ci:lint": "ng lint --format json --output-file /eslint/result.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "17.0.9",
    "@angular/common": "17.0.9",
    "@angular/compiler": "17.0.9",
    "@angular/core": "17.0.9",
    "@angular/forms": "17.0.9",
    "@angular/platform-browser": "17.0.9",
    "@angular/platform-browser-dynamic": "17.0.9",
    "@angular/router": "17.0.9",
    "angular-auth-oidc-client": "16.0.1",
    "ng-mocks": "14.11.0",
    "primeicons": "6.0.1",
    "primeng": "17.3.2",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "uuid": "9.0.1",
    "zone.js": "0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.10",
    "@angular-eslint/builder": "17.2.0",
    "@angular-eslint/eslint-plugin": "17.2.0",
    "@angular-eslint/eslint-plugin-template": "17.2.0",
    "@angular-eslint/schematics": "17.2.0",
    "@angular-eslint/template-parser": "17.2.0",
    "@angular/cli": "17.0.10",
    "@angular/compiler-cli": "17.0.9",
    "@badeball/cypress-cucumber-preprocessor": "18.0.6",
    "@cypress/schematic": "2.5.1",
    "@cypress/webpack-preprocessor": "6.0.0",
    "@testing-library/angular": "14.3.0",
    "@testing-library/cypress": "10.0.1",
    "@testing-library/jasmine-dom": "1.3.3",
    "@testing-library/user-event": "14.5.1",
    "@types/jasmine": "4.6.0",
    "@types/node": "20.8.6",
    "@types/testing-library__jasmine-dom": "1.3.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "cypress": "13.3.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jasmine": "4.1.3",
    "eslint-plugin-testing-library": "6.1.0",
    "jasmine-core": "5.1.1",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "primeflex": "3.3.1",
